{
    "name": "knplabs/php-json-schema-bundle",
    "description": "A Symfony bundle for knplabs/php-json-schema",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "KnpLabs\\JsonSchemaBundle\\": "src/"
        }
    },
    "authors": [
        {
            "name": "KnpLabs",
            "email": "hello@knplabs.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "knplabs/php-json-schema": "~0.1.0",
        "zircote/swagger-php": ">=4.4",
        "symfony/http-kernel": ">=6.0",
        "symfony/dependency-injection": ">=6.0",
        "symfony/config": ">=6.0",
        "swaggest/json-schema": ">=0.12.41"
    },
    "require-dev": {
        "vimeo/psalm": "5.x-dev"
    }
}
